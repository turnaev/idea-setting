    </context>
  </template>
  <template name="use" value="{% use '$TEMPLATE$' %}" description="{% use 'template' %}" toReformat="false" toShortenFQNames="true">
    <variable name="TEMPLATE" expression="" defaultValue="" alwaysStopAt="true" />
