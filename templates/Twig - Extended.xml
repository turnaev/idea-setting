    <variable name="VAR" expression="" defaultValue="&quot;var&quot;" alwaysStopAt="true" />
    <variable name="SEQUENCE" expression="" defaultValue="&quot;0..11&quot;" alwaysStopAt="true" />
    <variable name="STUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="MORESTUFF" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
